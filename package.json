{
	"name": "meoru-web",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:watch": "playwright test --ui",
		"test:e2e:coverage": "VITE_COVERAGE=true playwright test && nyc report --reporter=text --reporter=html"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.2",
		"@inlang/paraglide-js": "^2.9.0",
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.561.0",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-vercel": "^6.3.0",
		"@sveltejs/kit": "^2.49.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.2",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/svelte": "^5.3.1",
		"@types/node": "^25.0.3",
		"@vitest/coverage-v8": "^4.0.16",
		"bits-ui": "^2.15.4",
		"clsx": "^2.1.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^17.0.0",
		"jsdom": "^27.4.0",
		"lucide-svelte": "^0.562.0",
		"nyc": "^17.1.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"shadcn-svelte": "^1.1.0",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.52.0",
		"vite": "^7.3.0",
		"vite-plugin-istanbul": "^7.2.1",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.16.1",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-slot": "^1.2.4",
		"@vercel/analytics": "^1.6.1",
		"@vercel/speed-insights": "^1.3.1",
		"hono": "^4.11.3",
		"mode-watcher": "^1.1.0"
	},
	"engines": {
		"node": ">=24.11.1"
	},
	"packageManager": "pnpm@10.27.0"
}
